{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "auto"
      }
    ],
    ["@babel/preset-typescript", { "onlyRemoveTypeImports": true }],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ]
  ],
  "plugins": [
    "@emotion",
    [
      "@babel/plugin-transform-runtime",
      { "regenerator": true, "absoluteRuntime": true }
    ]
  ]
}
